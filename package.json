    "start": "gulp build & tsc & node ./dist/src/app.js",
    "test": "./node_modules/.bin/mocha --require ts-node/register ./tests/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barend-erasmus/shipping-system.git"
