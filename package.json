    "url": "https://github.com/barend-erasmus/shipping-system/issues"
  },
  "homepage": "https://github.com/barend-erasmus/shipping-system#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/es6-shim": "^0.31.37",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.4",
    "@types/sinon": "^5.0.1",
    "@types/sqlite3": "^3.1.3",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "istanbul": "^0.4.5",
