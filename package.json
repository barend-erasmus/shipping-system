    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.1",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  }
